{
    "name": "selmonal/laravel-simple-payment",
    "description": "A simple Qpay implementation for Laravel applications.",
    "keywords": [
        "selmonal",
        "laravel",
        "laravel-simple-payment"
    ],
    "homepage": "https://github.com/selmonal/laravel-simple-payment",
    "license": "MIT",
    "authors": [
        {
            "name": "Mygmarsuren Sedjav",
            "email": "miigaa.sv@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Selmonal\\LaravelSimplePayment\\": "src",
            "Selmonal\\LaravelSimplePayment\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Selmonal\\LaravelSimplePayment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Selmonal\\LaravelSimplePayment\\LaravelSimplePaymentServiceProvider"
            ],
            "aliases": {
                "LaravelSimplePayment": "Selmonal\\LaravelSimplePayment\\Facades\\LaravelSimplePayment"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}