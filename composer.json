{
    "name": "myagmarsurensedjav/simple-payment",
    "description": "A simple payment gateway implementation supporting multiple payment methods for Laravel applications.",
    "keywords": [
        "myagmarsurensedjav",
        "laravel",
        "simple-payment"
    ],
    "homepage": "https://github.com/myagmarsurensedjav/simple-payment",
    "license": "MIT",
    "authors": [
        {
            "name": "Mygmarsuren Sedjav",
            "email": "miigaa.sv@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "endroid/qr-code": "^5.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "laravel/pint": "^1.15.3",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MyagmarsurenSedjav\\SimplePayment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyagmarsurenSedjav\\SimplePayment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyagmarsurenSedjav\\SimplePayment\\SimplePaymentServiceProvider"
            ],
            "aliases": {
                "SimplePayment": "MyagmarsurenSedjav\\SimplePayment\\Facades\\SimplePayment"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
